#!/usr/bin/bash
#plankston

logo='\033[0;46m'
a='\033[30;0m'
n='\033[0m'
h='\033[32;1m'
m='\033[31;1m'
c='\033[36;1m'
p='\033[1;35m'
k='\033[33;1m'

ls /$PREFIX/bin && sleep 2 && sh .sshprem
clear
rm -rf SSH
rm -rf .plankston
sleep 1
cp .update $HOME
sleep 1
bash .authorBYplankston
#git clone https://github.com/mrben45/SSH
mv SSH .plankston
sh .plankston/.login
sh .plankston/.name0
clear
sleep 1
sh .plankston/.tamp
sh .plankston/.tamp2
sh .plankston/.tamp3
#echo $h
sh .plankston/data
sh .plankston/data2
echo $n
read -p " pilih menu : " men

case $men in
1)
echo "$h perintah benar$a"
sleep 1
rm -rf SSH
rm -rf .plankston
sleep 1
bash .authorBYplankston
#git clone https://github.com/mrben45/SSH
mv SSH .plankston
sleep 1
echo "$h [          ] 00% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■         ] 10% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■        ] 20% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■■       ] 30% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■■■      ] 40% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■■■■     ] 50% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■■■■■    ] 60% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■■■■■■   ] 70% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■■■■■■■  ] 80% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■■■■■■■■ ] 90% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 0.5
echo "$h [■■■■■■■■■■] 100% 𝙲𝚘𝚖𝚙𝚕𝚎𝚝𝚎𝚍"
sleep 3
cat .plankston/sg
sh .plankston/l
echo "$m tidak ada koneksi internet!"
echo ""
exit
;;

2)
echo "$h perintah benar$a"
sleep 1
rm -rf SSH
rm -rf .plankston
sleep 1
